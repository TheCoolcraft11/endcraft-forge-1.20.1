{
  "multipart": [
    {
      "when": {
        "half": "lower"
      },
      "apply": {
        "model": "endcraft:block/voidborn_portal_activator_lower"
      }
    },
    {
      "when": {
        "half": "upper",
        "on": "false"
      },
      "apply": {
        "model": "endcraft:block/voidborn_portal_activator_upper"
      }
    },
    {
      "when": {
        "half": "upper",
        "on": "true"
      },
      "apply": {
        "model": "endcraft:block/voidborn_portal_activator_upper_on"
      }
    }
  ]
}
